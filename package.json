{
  "name": "eslint-plugin-generate-test-id",
  "version": "0.0.0-development",
  "description": "Fill empty data-test-id attributes with nanoid.",
  "keywords": [
    "eslint",
    "eslint-plugin"
  ],
  "license": "MIT",
  "author": "matzkoh",
  "exports": {
    ".": "./index.cjs",
    "./rules/*": "./rules/*.cjs",
    "./package.json": "./package.json"
  },
  "main": "./index.cjs",
  "files": [
    "configs",
    "rules",
    "index.cjs"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "run-p \"lint:*\"",
    "lint:docs": "npm run gen:docs -- --check",
    "lint:js": "eslint . --ext js,cjs,mjs",
    "lint:prettier": "prettier -c .",
    "lint:spell": "cspell --gitignore --no-progress .",
    "test": "c8 --reporter=lcov mocha tests --recursive",
    "gen": "run-s gen:*",
    "gen:docs": "eslint-doc-generator"
  },
  "dependencies": {
    "nanoid": "^3.3.7"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "@tsconfig/strictest": "2.0.5",
    "@types/eslint": "8.56.10",
    "@types/node": "20.14.2",
    "@typescript-eslint/types": "7.13.1",
    "c8": "10.1.2",
    "cspell": "8.8.4",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-doc-generator": "1.7.1",
    "eslint-plugin-eslint-plugin": "6.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-unicorn": "54.0.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "mocha": "10.4.0",
    "npm-run-all2": "6.2.0",
    "prettier": "3.3.2",
    "prettier-plugin-packagejson": "2.5.0",
    "semantic-release": "24.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  }
}
