{
  "name": "eslint-plugin-generate-test-id",
  "version": "0.0.0-development",
  "description": "Fill empty data-test-id attributes with nanoid.",
  "keywords": [
    "eslint",
    "eslint-plugin"
  ],
  "license": "MIT",
  "author": "matzkoh",
  "exports": {
    ".": "./index.cjs",
    "./rules/*": "./rules/*.cjs",
    "./package.json": "./package.json"
  },
  "main": "./index.cjs",
  "files": [
    "configs",
    "rules",
    "index.cjs"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "run-p \"lint:*\"",
    "lint:docs": "npm run gen:docs -- --check",
    "lint:js": "eslint . --ext js,cjs,mjs",
    "lint:spell": "cspell --gitignore --no-progress .",
    "test": "c8 --reporter=lcov mocha tests --recursive",
    "gen": "run-s gen:*",
    "gen:docs": "eslint-doc-generator"
  },
  "dependencies": {
    "nanoid": "^3.3.6"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.2",
    "@tsconfig/strictest": "2.0.2",
    "@types/eslint": "8.44.8",
    "@types/node": "20.10.2",
    "@typescript-eslint/types": "6.13.1",
    "c8": "8.0.1",
    "cspell": "8.1.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-doc-generator": "1.6.1",
    "eslint-plugin-eslint-plugin": "5.1.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-unicorn": "49.0.0",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "mocha": "10.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.0",
    "prettier-plugin-packagejson": "2.4.6",
    "semantic-release": "22.0.8"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  }
}
